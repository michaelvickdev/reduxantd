{
  "name": "webpack-library-starter",
  "version": "0.3.0",
  "description": "Produce universal library with webpack and es6",
  "main": "./lib/index.js",
  "jsnext:main": "./es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhdmitry/redux-form-antd"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:es",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:lib": "babel src --out-dir lib",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "antd": "^2.10.4",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "react": "*",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-form": "^6.7.0",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.68.5"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.0.0",
    "@storybook/react": "^3.0.0",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "cross-env": "^5.0.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "react": "^15.5.4",
    "react-dom": "*",
    "yargs": "7.1.0"
  },
  "keywords": [
    "redux-form",
    "antd",
    "bindings"
  ],
  "author": "zherebko dmitry",
  "license": "MIT",
  "bugs": {}
}